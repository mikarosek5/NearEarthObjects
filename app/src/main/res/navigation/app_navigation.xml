<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/app_navigation"
            app:startDestination="@id/pictureOfDayFragment">

    <fragment android:id="@+id/pictureOfDayFragment"
              android:name="eu.invest.klk.neadearthobjects.ui.picture_of_day.PictureOfDayFragment"
              android:label="Today" tools:layout="@layout/picture_of_day_fragment">
        <action android:id="@+id/action_pictureOfDayFragment_to_bigPictureFragment"
                app:destination="@id/bigPictureFragment" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/neoListFragment"
              android:name="eu.invest.klk.neadearthobjects.ui.neo.list.NeoListFragment"
              tools:layout="@layout/neo_list_fragment">
        <action android:id="@+id/action_neoListFragment_to_errorFragment" app:destination="@id/errorFragment"
                app:exitAnim="@anim/nav_default_exit_anim" app:enterAnim="@anim/nav_default_enter_anim"
                app:popEnterAnim="@anim/nav_default_enter_anim" app:popExitAnim="@anim/nav_default_exit_anim"
                app:popUpTo="@+id/pictureOfDayFragment"/>
    </fragment>
    <fragment android:id="@+id/spaceXListFragment"
              android:name="eu.invest.klk.neadearthobjects.ui.spacex.launcheslist.SpaceXListFragment"
              android:label="SpaceX" tools:layout="@layout/space_xlist_fragment">
        <action android:id="@+id/action_spaceXListFragment_to_spaceXDetails" app:destination="@id/spaceXDetails"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim" app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/errorFragment"
              android:name="eu.invest.klk.neadearthobjects.ui.error_fragment.ErrorFragment"
              android:label="Oooops">
        <action android:id="@+id/action_errorFragment_to_pictureOfDayFragment"
                app:destination="@id/pictureOfDayFragment" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" app:popEnterAnim="@anim/nav_default_enter_anim"
                app:popExitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/bigPictureFragment"
              android:name="eu.invest.klk.neadearthobjects.ui.picture_of_day.big_picture.BigPictureFragment"
              android:label="big_picture_fragment" tools:layout="@layout/big_picture_fragment"/>
    <fragment android:id="@+id/spaceXDetails"
              android:name="eu.invest.klk.neadearthobjects.ui.spacex.next_launch.SpaceXDetails"
              android:label="space_xdetails_fragment" tools:layout="@layout/space_xdetails_fragment">
        <argument android:name="launch_id" app:argType="integer"/>
    </fragment>
</navigation>